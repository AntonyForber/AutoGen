{
    "name": "antony-forber/autogen",
    "description": "Visual generator for Laravel",
    "authors": [
        {
            "name": "Antony Forber",
            "email": "forber@mail.ua"
        }
    ],
    "autoload": {
        "psr-4": { "AntonyForber\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "AntonyForber\\AutoGen\\AutoGenProvider"
            ]
        }
    },
    "post-update-cmd": [
        "php artisan vendor:publish --tag=autogen --force"
    ]
}